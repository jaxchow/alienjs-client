{
  "name": "alienjs-client",
  "version": "0.0.1",
  "description": "admin manager for alienjs",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive --growl",
    "test:all": "mocha --compilers js:babel-core/register --recursive **/spec/*.spec.js",
    "test:watch": "npm run test:all -- --watch",
    "test:books": "npm test -- src/app_modules/Books/**/spec/*.spec.js",
    "test:users": "npm test -- src/app_modules/Users/**/spec/*.spec.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js --devtool eval --progress --colors --watch --hot --inline --open",
    "start": "webpack-dev-server --config webpack.config.js --devtool eval"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jaxchow/alienjs-client.git"
  },
  "keywords": [
    "web",
    "client"
  ],
  "author": "jaxchow@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaxchow/alienjs-client/issues"
  },
  "homepage": "https://github.com/jaxchow/alienjs-client#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.0.12",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.8.1",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-slider-monitor": "^1.0.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "fixed-data-table": "^0.6.0",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.7",
    "react-layout-pane": "^0.1.16",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "react-sidenav": "0.0.15",
    "react-style": "git+https://github.com/js-next/react-style.git#0.14",
    "redux": "^3.3.1",
    "redux-form": "^4.1.8",
    "redux-form-validation": "0.0.6",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "webpack-html-plugin": "^0.1.1"
  }
}
